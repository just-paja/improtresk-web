{
  "name": "improtresk-web",
  "version": "0.1.0",
  "description": "Website app for Improt≈ôesk festival, Czech improvisation festival",
  "main": "dist/server.js",
  "private": true,
  "scripts": {
    "dev": "babel-node ./src/server/main",
    "build": "npm run build:clean && npm run build:client && npm run build:server",
    "build:clean": "rimraf dist",
    "build:client": "webpack --config webpack/client.babel.js",
    "build:server": "webpack --config webpack/server.babel.js",
    "start": "node ./dist/server.js",
    "test:production": "npm run build && node dist/server.js",
    "test:unit": "mocha --compilers babel-register -r mocha.js --recursive",
    "test:watch": "npm run test:unit -- -w --watch-extensions js,jsx,css",
    "test": "node scripts/test.js --env=jsdom"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/just-paja/improtresk-web.git"
  },
  "author": "just-paja <just.paja@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/just-paja/improtresk-web/issues"
  },
  "homepage": "https://improtresk.cz",
  "dependencies": {
    "accept-language-parser": "^1.5.0",
    "body-parser": "^1.18.3",
    "bootstrap": "^4.0.0",
    "bootswatch": "^4.0.0",
    "classnames": "^2.2.5",
    "cookie-parser": "^1.4.4",
    "crossing": "^1.2.1",
    "css-loader": "^0.28.0",
    "css-modules-require-hook": "^4.0.6",
    "diacritics": "^1.3.0",
    "exenv": "^1.2.2",
    "express": "^4.16.4",
    "express-device": "^0.4.2",
    "font-awesome": "^4.7.0",
    "form-urlencoded": "^2.0.4",
    "history": "^4.7.2",
    "isomorphic-fetch": "^2.2.1",
    "js-cookie": "^2.1.4",
    "json-loader": "^0.5.4",
    "moment": "^2.21.0",
    "moment-timezone": "^0.5.14",
    "postcss": "^6.0.19",
    "postcss-cssnext": "^3.1.0",
    "postcss-import": "^11.1.0",
    "postcss-loader": "^2.1.0",
    "prop-types": "^15.6.0",
    "query-string-manipulator": "^1.0.4",
    "react": "16.2.0",
    "react-dom": "^16.2.0",
    "react-fontawesome": "^1.5.0",
    "react-ga": "^2.5.0",
    "react-google-maps": "^9.4.5",
    "react-helmet": "^5.0.3",
    "react-images": "^0.5.2",
    "react-localize-redux": "^2.17.5",
