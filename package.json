    "react-router-bootstrap": "^0.24.4",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^4.0.8",
    "react-saga-rest": "^1.1.8",
    "react-select": "^1.0.0-rc.3",
    "reactstrap": "^5.0.0",
    "redux": "^3.6.0",
    "redux-form": "^7.3.0",
    "redux-logger": "^3.0.1",
    "redux-saga": "^0.16.0",
    "redux-saga-restart": "^0.1.0",
    "redux-saga-routines": "^2.2.3",
    "reselect": "^3.0.1",
    "revalidator": "^0.3.1",
